<launch>
  <env name="ROSCONSOLE_FORMAT" value="[${severity}${node}]: [${time}] ${message}"/>
  
  <node name="Low_Level_Node" pkg="AutoPinball" type="run_low_level.py" output="screen" launch-prefix="gnome-terminal --command"/>
  <node name="GUI_Node" pkg="AutoPinball" type="gui.py" output="screen"/>
  <node name="Keyboard_Node" pkg="AutoPinball" type="KeyboardFlippers.py" output="screen" launch-prefix="gnome-terminal --command"/>
  
  <node name="Arduino_Node" pkg="rosserial_arduino" type="serial_node.py" args="/dev/ttyACM0" required="true" output="screen"/>

</launch>
